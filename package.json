  },
  "lint-staged": {
    "src/**/*.{ts,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "lint": "tslint --type-check --project './tsconfig.json' -e 'node_modules/**'",
    "prettier:format": "prettier --write \"src/**/*.{ts,css,scss}\"",
    "lint:fix": "yarn run lint -- --fix",
    "ngc": "ngc -p tsconfig-aot.json",
    "cleanup": "rimraf build/{aot,www}",
    "clean-www": "rimraf build//www/app/{src,build/}",
    "start": "yarn run webpack:dev",
    "serve": "yarn run start",
    "build": "yarn run webpack:prod",
    "test": "karma start src/test/javascript/karma.conf.js",
    "test:watch": "yarn test -- --watch",
    "webpack:dev": "yarn run webpack-dev-server -- --config webpack/webpack.dev.js --progress --inline --hot --profile --port=9060 --watch-content-base --env.stats=minimal",
    "webpack:local": "yarn run webpack-dev-server -- --config webpack/webpack.local.js --progress --inline --hot --profile --port=9060 --watch-content-base --env.stats=minimal",
    "webpack:localprod": "yarn run webpack-dev-server -- --config webpack/webpack.localprod.js --progress --inline --hot --profile --port=9060 --watch-content-base --env.stats=minimal",
    "webpack:local-iss": "yarn run webpack-dev-server -- --config webpack/webpack.local-iss.js --progress --inline --hot --profile --port=9060 --watch-content-base --env.stats=minimal",
    "webpack:local-ng": "yarn run webpack-dev-server -- --config webpack/webpack.local-ng.js --progress --inline --hot --profile --port=9060 --watch-content-base --env.stats=minimal",
    "webpack:local-all": "yarn run webpack-dev-server -- --config webpack/webpack.local-all.js --progress --inline --hot --profile --port=9060 --watch-content-base --env.stats=minimal",
    "webpack:iss": "yarn run webpack-dev-server -- --config webpack/webpack.iss.js --progress --inline --hot --profile --port=9060 --watch-content-base --env.stats=minimal",
    "webpack:build:main": "yarn run webpack -- --config webpack/webpack.dev.js --progress --profile --env.stats=normal",
    "webpack:build": "yarn run cleanup && yarn run webpack:build:main",
    "webpack:prod:main": "yarn run webpack -- --config webpack/webpack.prod.js --progress --profile",
    "webpack:prod": "yarn run cleanup && yarn run webpack:prod:main && yarn run clean-www",
    "webpack:test": "yarn run test",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  }
}
